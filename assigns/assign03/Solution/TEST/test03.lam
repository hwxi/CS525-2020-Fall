fun main() =
(
  let
      val arr = (1, 2)
  in
      let
          val sum = lam(x: int*int) => x.0 + x.1
      in
          let
              val apply = lam(x: (int*int->int)*(int*int)) => (x.0)(x.1)
          in
              let
                  val args = (sum, arr)
              in
                  apply(args)
              end
          end
      end
  end
)

